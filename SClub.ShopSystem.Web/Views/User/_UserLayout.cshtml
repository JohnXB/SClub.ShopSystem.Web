<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Css/style.css")
    @Styles.Render("~/Css/animate.css")
    @Styles.Render("~/Css/bootstrap.css")
    @Styles.Render("~/Css/icomoon.css")
    @Styles.Render("~/Css/magnific-popup.css")
    @Styles.Render("~/Css/User/UserLayout.css")
        @Scripts.Render("~/Js/jquery-1.11.0.min.js")
</head>
<body>
    @Html.Partial("_UserHeader")
    <div id="head">&nbsp;</div>
        <div class="center">
            <div class="left" >
                <ul class="layui-nav layui-nav-tree " lay-filter="test">
                    <!-- 侧边导航: <ul class="layui-nav layui-nav-tree layui-nav-side"> -->
                    <li style="margin-bottom:15px;font-size:15px;color:#01AAED">菜单</li>
                    <li class="layui-nav-item"><a href="/User/PersonCenter">个人中心</a></li>
                    <li class="layui-nav-item"><a href="/User/AddressInfo">收货地址</a></li>
                    <li class="layui-nav-item"><a href="/User/ShoppingCart">购物车</a></li>
                    <li class="layui-nav-item layui-nav-itemed">
                        <a href="javascript:;">物流管理</a>
                        <dl class="layui-nav-child">
                            <dd><a href="/User/Deliver">待发货</a></dd>
                            <dd><a href="/User/ReciptGoods">待收货</a></dd>
                            <dd><a href="/User/UserBoughtRecord">购买记录</a></dd>
                        </dl>
                    </li>

                </ul>
            </div>
            
            <div class="right">
                @RenderBody()
            </div>
            
        </div>
    @Html.Partial("_Footer")

    @Scripts.Render("~/Js/jquery.easing.1.3.js")
    @Scripts.Render("~/Js/bootstrap.min.js")
    @Scripts.Render("~/Js/jquery.waypoints.min.js")
    @Scripts.Render("~/Js/jquery.countTo.js")
    @Scripts.Render("~/Js/jquery.magnific-popup.min.js")
    @Scripts.Render("~/Js/magnific-popup-options.js")
    @Scripts.Render("~/Js/jquery.stellar.min.js")
    @Scripts.Render("~/Js/main.js")
    @Scripts.Render("~/Js/modernizr-2.6.2.min.js")
</body>
</html>
